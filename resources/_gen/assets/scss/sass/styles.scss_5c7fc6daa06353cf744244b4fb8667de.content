* {
  background-color: #fff;
  font-family: "Poppins", sans-serif;
  font-size: 16px; }

ul {
  padding: 0;
  list-style-type: none; }

a {
  text-decoration: none;
  color: inherit; }

.container .row {
  padding: 15px 0px; }

.btn {
  border: 1px #f28482 solid;
  padding: 5px; }

.menu li {
  display: inline; }

.page-title {
  font-size: 250%; }
